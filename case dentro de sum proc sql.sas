


DATA INPUTANDO_DADOS ;
INPUT ID NOME $;
DATALINES;
3 JONATHAN
2 CARLOS
1 CLEBER
6 PEDRO
9 JEFERSON
10 ANDRE 
;
RUN;

proc sql;
	CREATE TABLE CASE_SUM AS SELECT

/* MOSTRA AS QTDS DE VEZES QUE TIVEMOS UM ID <= 6 */
		SUM(CASE WHEN ID <= 6 THEN 1 ELSE 0 END) AS SOMA_IDS
	FROM INPUTANDO_DADOS
	;
QUIT;


	