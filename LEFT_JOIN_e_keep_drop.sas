DATA DADOS;
INPUT ID NOME $ NUM;
DATALINES;
100 JONATHAN 10
200 CARLOS 15
300 ANA 30
500 PEDRO 50
700 ZELTA 70
;
RUN;

DATA PRODUTO;
INPUT ID NOMEPRODUTO $;
DATALINES;
100 GILETE
200 CARNE
300 PÃO


; 
RUN;

PROC SQL;
	CREATE TABLE UNIAO_LEFTJOIN AS SELECT	
		A.ID
		,A.NOME
		,B.NOMEPRODUTO
	FROM DADOS AS A LEFT JOIN
	PRODUTO AS B ON A.ID = B.ID
	;
QUIT;


DATA KEEPZIN;
SET UNIAO_LEFTJOIN(KEEP=NOME NOMEPRODUTO OBS= 2);
RUN;